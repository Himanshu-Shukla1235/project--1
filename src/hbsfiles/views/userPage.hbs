    {{>Footer}}

    {{!-- ----------------------------- Script ---------------------------- --}}
    <script src="userPage.js"></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        AOS.init({
            offset: 300, 
            delay: 0, 
            duration: 1000, 
            easing: 'ease', 
            once: false, 
            mirror: false, 
            anchorPlacement: 'top-bottom',
        });
    </script>
